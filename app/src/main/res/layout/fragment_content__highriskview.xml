<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="350sp"
    android:layout_height="550sp"
    android:background="#FFFFFF"
    tools:context=".Content.Content_HighRiskView">

    <ScrollView
        android:layout_width="350sp"
        android:layout_height="500sp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#673AB7"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="#673AB7"
                    android:text="   고위험 임신 위험도 점수 산출 기준"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/x_btn"
                    android:layout_width="30sp"
                    android:layout_height="28sp"
                    android:background="#00FFFFFF"
                    android:text="❌"
                    android:textSize="10sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="7sp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="섭취칼로리 : 500kcal     \n소모 칼로리 : 500kcal \n고위험 임신 테스트 점수 : 20점"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#D6BEFF"
                    android:text=" 평가기준 1) 섭취 칼로리"
                    android:textSize="14sp"
                    android:textStyle="bold" />

                <Space
                    android:layout_width="match_parent"
                    android:layout_height="5sp" />

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="*  임신부 권장 섭취 칼로리"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv1_1_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
                    android:text="01~12주차 : 2100kcal"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv1_1_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
                    android:text="13~28주차 : 2340kcal"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv1_1_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="29~40주차 : 2452kcal"
                    android:background="#FFFFFF"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="\n* 점수측정"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv1_2_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="권장섭취칼로리 ±10%  = 1점"
                    android:background="#FFFFFF"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv1_2_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="권장섭취칼로리 ±15% ~ ±10%  = 3점"
                    android:background="#FFFFFF"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv1_2_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="권장섭취칼로리 ±20% ~ ±15%  = 5점"
                    android:background="#FFFFFF"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv1_2_4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="권장섭취칼로리 ±20% ~  = 7점"
                    android:background="#FFFFFF"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textView145"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/textView30"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#D6BEFF"
                    android:text=" 평가기준 2) 소모 칼로리"
                    android:textSize="14sp"
                    android:textStyle="bold" />

                <Space
                    android:layout_width="match_parent"
                    android:layout_height="5sp" />

                <TextView
                    android:id="@+id/textView31"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="* 여성 기초대사량 (kcal) = \n\t\t\t\t\t\t655.1+(9.56*체중)+(1.85*키)-(4.68*나이)"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv2_1_1"
                    android:layout_width="match_parent"
                    android:background="#FFFFFF"
                    android:layout_height="wrap_content"
                    android:text="01~12주차 : 여성 기초대사량에 + 300kcal "
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv2_1_2"
                    android:layout_width="match_parent"
                    android:background="#FFFFFF"
                    android:layout_height="wrap_content"
                    android:text="13~28주차 : 여성 기초대사량에 + 400kcal "
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv2_1_3"
                    android:layout_width="match_parent"
                    android:background="#FFFFFF"
                    android:layout_height="wrap_content"
                    android:text="29~40주차 : 여성 기초대사량에 + 500kcal "
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textView124"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="* \n하루 소모 칼로리 = 섭취칼로리 - (기초대사량 + 개인소모 칼로리)"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView126"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="\n* 점수측정"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv2_2_1"
                    android:background="#FFFFFF"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="하루소모칼로리 : -200kcal ~ 200kcal= 1점"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv2_2_2"
                    android:layout_width="match_parent"
                    android:background="#FFFFFF"
                    android:layout_height="wrap_content"
                    android:text="하루소모칼로리 : -400kcal ~ -201kcal / 201kcal ~ 400kcal = 3점"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv2_2_3"
                    android:layout_width="match_parent"
                    android:background="#FFFFFF"
                    android:layout_height="wrap_content"
                    android:text="하루소모칼로리 : -600kcal ~ -401kcal / 401kcal ~ 600kcal = 5점"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv2_2_4"
                    android:background="#FFFFFF"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="하루소모칼로리 : ~ -600kcal  / 600kcal ~ = 7점"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textView146"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/textView132"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#D6BEFF"
                    android:text=" 평가기준 3) 고위험 임신군 위험도 테스트 점수 "
                    android:textSize="14sp"
                    android:textStyle="bold" />

                <Space
                    android:layout_width="match_parent"
                    android:layout_height="5sp" />

                <TextView
                    android:id="@+id/tv3_1_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
                    android:text="안전(0점~39점)  = 1점"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv3_1_2"
                    android:layout_width="match_parent"
                    android:background="#FFFFFF"
                    android:layout_height="wrap_content"
                    android:text="주의(40점~59점)  = 7점"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv3_1_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
                    android:text="경고(60점~79점)  = 10점"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv3_1_4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
                    android:text="위험 (80점 이상)  = 20점"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textView137"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#C19DFF"
                    android:text="최종 점수 산출"
                    android:textAlignment="center"
                    android:textSize="14sp"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView144"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:text="총점 |"
                        android:textAlignment="textEnd" />

                    <TextView
                        android:id="@+id/tv_score"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="10"
                        android:textAlignment="textEnd"
                        android:textColor="#673AB7"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textView149"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:text=" / 25"
                        android:textAlignment="textStart" />

                    <TextView
                        android:id="@+id/textView150"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text=" " />
                </LinearLayout>

                <TextView
                    android:id="@+id/tv4_1_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
                    android:text="25점 이상 = 위험"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv4_1_2"
                    android:layout_width="match_parent"
                    android:background="#FFFFFF"
                    android:layout_height="wrap_content"
                    android:text="19점 ~ 24점  = 경고"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv4_1_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
                    android:text="13점 ~ 18점  = 주의"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv4_1_4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
                    android:text="7점 ~ 12점  = 양호"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv4_1_5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
                    android:text="6점 이하  = 매우양호"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textStyle="bold" />

            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</FrameLayout>